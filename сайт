<script>
function showInfo(planet){
    let info = "";
    switch(planet){
        case "–ú–µ—Ä–∫—É—Ä–∏–π":
            info = "–°–∞–º–∞—è –º–∞–ª–µ–Ω—å–∫–∞—è –ø–ª–∞–Ω–µ—Ç–∞ –≤ –°–æ–ª–Ω–µ—á–Ω–æ–π —Å–∏—Å—Ç–µ–º–µ";
            break;
        case "–í–µ–Ω–µ—Ä–∞":
            info = "–í–µ–Ω–µ—Ä–∞ —è–≤–ª—è–µ—Ç—Å—è —Å–∞–º–æ–π –≥–æ—Ä—è—á–µ–π –ø–ª–∞–Ω–µ—Ç–æ–π";
            break;
        case "–ó–µ–º–ª—è":
            info = "–ü–ª–∞–Ω–µ—Ç–∞, –Ω–∞ –∫–æ—Ç–æ—Ä–æ–π –º—ã –∂–∏–≤—ë–º";
            break;
        case "–ú–∞—Ä—Å":
            info = "–°–∞–º–∞—è –±–ª–∏–∑–∫–∞—è –∫ –ó–µ–º–ª–µ –ø–ª–∞–Ω–µ—Ç–∞ –°–æ–ª–Ω–µ—á–Ω–æ–π —Å–∏—Å—Ç–µ–º—ã";
            break;
        case "–Æ–ø–∏—Ç–µ—Ä":
            info = "–°–∞–º–∞—è –±–æ–ª—å—à–∞—è –≤ –°–æ–ª–Ω–µ—á–Ω–æ–π —Å–∏—Å—Ç–µ–º–µ";
            break;
        case "–°–∞—Ç—É—Ä–Ω":
            info = "–ü–ª–∞–Ω–µ—Ç–∞ —Å –∫–æ–ª—å—Ü–∞–º–∏ –≤–æ–∫—Ä—É–≥ —Å–≤–æ–µ–π –æ—Å–∏";
            break;
        case "–£—Ä–∞–Ω":
            info = "–û–¥–Ω–∞ –∏–∑ —Å–∞–º—ã—Ö —Ö–æ–ª–æ–¥–Ω—ã—Ö –ø–ª–∞–Ω–µ—Ç –°–æ–ª–Ω–µ—á–Ω–æ–π —Å–∏—Å—Ç–µ–º—ã";
            break;
        case "–ù–µ–ø—Ç—É–Ω":
            info = "–ü–ª–∞–Ω–µ—Ç–∞, –∫–æ—Ç–æ—Ä—É—é –æ–±–Ω–∞—Ä—É–∂–∏–ª–∏ —Å –ø–æ–º–æ—â—å—é –º–∞—Ç–µ–º–∞—Ç–∏—á–µ—Å–∫–∏—Ö –≤—ã—á–∏—Å–ª–µ–Ω–∏–π";
